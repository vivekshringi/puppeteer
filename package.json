{
  "name": "example",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^25.2.3",
    "@types/puppeteer": "^3.0.0",
    "eslint": "^7.11.0",
    "jest": "^26.0.1",
    "puppeteer": "^5.3.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "test": "jest --color",
    "test-event": "jest event.test --color",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|css|html)\"",
    "build": "tsc -b"
  },
  "jest": {
    "cacheDirectory": "node_modules/.jest-cache",
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./setup.ts"
    ],
    "testEnvironment": "node",
    "testRegex": "^(?!.*\\.d.ts$).*\\.test\\.ts$"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "prettier": "^2.1.2"
  }
}
